{
  "name": "ariel",
  "version": "2.4.3",
  "main": "build/index",
  "author": "AstrielDivision",
  "contributors": [
    {
      "name": "TMUniversal",
      "email": "me@tmuniversal.eu",
      "url": "https://tmuniversal.eu/"
    }
  ],
  "license": "Apache-2.0",
  "private": true,
  "description": "Discord bot using @sapphire/framework",
  "scripts": {
    "start": "NODE_ENV=\"production\" && node .",
    "start:dev": "node .",
    "lint": "eslint --cache --report-unused-disable-directives --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "eslint --cache --report-unused-disable-directives --ext .ts src/ --fix",
    "build": "yarn clean && yarn build:ts",
    "build:ts": "tsc -b src",
    "clean": "rimraf ./build && mkdir build",
    "commit": "git-cz",
    "postinstall": "husky install .github/husky",
    "upgrade": "yarn upgrade-interactive",
    "contrib:add": "all-contributors add",
    "contrib:gen": "all-contributors generate",
    "contrib:check": "all-contributors check",
    "prettiest": "prettier --write src && yarn lint:fix"
  },
  "devDependencies": {
    "@astriel-division/eslint-config": "1.2.0",
    "@astriel-division/ts-config": "1.0.5",
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "@tmware/prettier-config": "1.0.3",
    "@types/app-root-path": "1.2.4",
    "@types/convict": "6.1.1",
    "@types/crypto-js": "4.0.2",
    "@types/node": "16.9.4",
    "@types/prettier": "2.4.1",
    "all-contributors-cli": "6.20.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.0.1",
    "husky": "7.0.2",
    "lint-staged": "11.2.3",
    "prettier": "2.4.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "tslib": "2.3.1",
    "typescript": "4.4.4"
  },
  "engines": {
    "node": "^16",
    "npm": "^8.0.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@aero/ksoft": "4.0.0-rc1",
    "@aero/sanitizer": "1.2.0",
    "@sapphire/decorators": "3.1.2",
    "@sapphire/discord-utilities": "2.2.1",
    "@sapphire/discord.js-utilities": "3.2.2",
    "@sapphire/fetch": "2.0.1",
    "@sapphire/framework": "2.1.3",
    "@sapphire/plugin-api": "3.0.1",
    "@sapphire/plugin-i18next": "2.0.0-pr-145.4dbd0994.0",
    "@sapphire/plugin-subcommands": "2.0.1",
    "@sapphire/type": "2.1.0",
    "@sapphire/utilities": "3.0.4",
    "@sentry/node": "6.13.3",
    "@tmware/status-rotate": "1.2.9",
    "app-root-path": "3.0.0",
    "bufferutil": "4.0.5",
    "convict": "6.2.1",
    "crypto-js": "4.1.1",
    "dayjs": "1.10.7",
    "discord-api-types": "0.24.0",
    "discord.js": "13.2.0",
    "mongoose": "6.0.11",
    "nanoid": "3.1.30",
    "owofire": "1.0.4",
    "utf-8-validate": "5.0.7"
  },
  "imports": {
    "#lib/Structures/Command": "./build/lib/Structures/Command.js",
    "#lib/Structures/commands/CanvasCommand": "./build/lib/Structures/commands/CanvasCommand.js",
    "#lib/Structures/commands/RedditCommand": "./build/lib/Structures/commands/RedditCommand.js",
    "#types": "/build/lib/Models/types.js",
    "#lib/*": "./build/lib/*.js",
    "#setup": "./build/lib/setup.js",
    "#languages": "./build/lib/i18n/LanguageKeys/index.js"
  },
  "lint-staged": {
    "./src/**/*.ts": "yarn prettiest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AstrielDivision/Ariel.git"
  }
}
