{
  "name": "astraea",
  "version": "2.0.3",
  "main": "build/index",
  "author": "ArtieFuzzz#8298",
  "contributors": [
    "TMUniversal <me@tmuniversal.eu> (https://tmuniversal.eu/)"
  ],
  "license": "Apache-2.0",
  "private": true,
  "description": "Discord bot using @sapphire/framework",
  "scripts": {
    "start": "NODE_ENV=\"production\" && node .",
    "start:dev": "node .",
    "lint": "eslint --cache --report-unused-disable-directives --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "eslint --cache --report-unused-disable-directives --ext .ts src/ --fix",
    "build": "yarn clean && yarn build:ts",
    "build:ts": "tsc -b src",
    "clean": "rimraf ./build && mkdir build",
    "commit": "git-cz",
    "postinstall": "husky install .github/husky",
    "upgrade": "yarn upgrade-interactive",
    "contrib:add": "all-contributors add",
    "contrib:gen": "all-contributors generate",
    "contrib:check": "all-contributors check",
    "prettiest": "prettier --write src && yarn lint:fix"
  },
  "devDependencies": {
    "@astriel-division/eslint-config": "1.1.6",
    "@astriel-division/ts-config": "1.0.5",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@tmware/prettier-config": "1.0.3",
    "@types/app-root-path": "1.2.4",
    "@types/convict": "6.1.1",
    "@types/crypto-js": "4.0.2",
    "@types/node": "16.7.10",
    "@types/prettier": "2.3.2",
    "all-contributors-cli": "6.20.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "husky": "7.0.2",
    "lint-staged": "11.1.2",
    "prettier": "2.3.2",
    "rimraf": "3.0.2",
    "tslib": "2.3.1",
    "typescript": "4.4.2"
  },
  "engines": {
    "node": "^16",
    "npm": "^7"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@aero/ksoft": "4.0.0-rc1",
    "@aero/sanitizer": "1.2.0",
    "@sapphire/decorators": "^3.0.0-next.c4f992f5.0",
    "@sapphire/discord-utilities": "2.1.5",
    "@sapphire/discord.js-utilities": "^3.0.0-next.c4f992f5.0",
    "@sapphire/fetch": "^2.0.0-next.c4f992f5.0",
    "@sapphire/framework": "^2.0.0-next.9f81b7bb.0",
    "@sapphire/plugin-subcommands": "^2.0.0-next.3bd3e99.0",
    "@sapphire/type": "2.1.0",
    "@sapphire/utilities": "2.0.1",
    "@sentry/node": "6.12.0",
    "@tmware/status-rotate": "1.2.8",
    "app-root-path": "3.0.0",
    "bufferutil": "4.0.3",
    "colorette": "1.3.0",
    "convict": "6.2.0",
    "crypto-js": "4.1.1",
    "dayjs": "1.10.6",
    "discord.js": "13.1.0",
    "mongoose": "6.0.4",
    "owofire": "1.0.4",
    "utf-8-validate": "5.0.5"
  },
  "imports": {
    "#lib/Structures/BaseCommand": "./build/lib/Structures/Command.js",
    "#lib/Structures/commands/CanvasCommand": "./build/lib/Structures/commands/CanvasCommand.js",
    "#lib/Structures/commands/RedditCommand": "./build/lib/Structures/commands/RedditCommand.js",
    "#types": "/build/lib/Models/types.js",
    "#lib/*": "./build/lib/*.js"
  },
  "lint-staged": {
    "./src/**/*.ts": "yarn prettiest"
  }
}
