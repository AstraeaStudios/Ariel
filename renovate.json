{
  "extends": ["config:base"],
  "baseBranches": ["starlight"],
  "automerge": "true",
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch"],
      "matchCurrentVersion": "!/^0/",
      "automerge": true
    }
  ],
  "lockFileMaintenance": {
    "enabled": true,
    "automerge": true
  },
  "automergeStrategy": "rebase"
}
