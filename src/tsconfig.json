{
  "extends": "@astriel-division/ts-config/src/tsconfig.json",
  "compilerOptions": {
    "outDir": "../build",
    "rootDir": ".",
    "baseUrl": ".",
    "paths": {
      "#lib/Structures/BaseCommand": ["lib/Structures/Command.ts"],
      "#lib/Structures/commands/CanvasCommand": ["lib/Structures/commands/CanvasCommand.ts"],
      "#lib/Structures/commands/RedditCommand": ["lib/Structures/commands/RedditCommand.ts"],
      "#types": ["lib/Types"],
      "#lib/*": ["lib/*"],
      "#setup": ["lib/setup.ts"],
      "#languages": ["lib/i18n/LanguageKeys/index"]
    },
    "strictNullChecks": false,
    "resolveJsonModule": true
  },
  "include": [".", "./**/*.json"],
  "exclude": ["tsconfig.json"]
}
