{
  "extends": "@astriel-division/ts-config",
  "compilerOptions": {
    "outDir": "../build",
    "rootDir": ".",
    "baseUrl": ".",
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "paths": {
      "#lib/Structures/Command": ["lib/Structures/Command.ts"],
      "#lib/Structures/commands/CanvasCommand": ["lib/Structures/commands/CanvasCommand.ts"],
      "#lib/Structures/commands/RedditCommand": ["lib/Structures/RedditCommand.ts"],
      "#types": ["lib/Types/index.ts"],
      "#lib/*": ["lib/*"],
      "#setup": ["lib/setup.ts"],
      "#languages": ["lib/i18n/LanguageKeys/index"],
      "#util": ["lib/util/index"]
    },
    "strictNullChecks": false,
    "resolveJsonModule": true
  },
  "include": [".", "./**/*.json"],
  "exclude": ["tsconfig.json"]
}
